page {
  padding: 20rpx;
  background-color: #dedede;
}
.search-row {
  height: 60rpx;
  display: flex;
}
.search-row input {
  flex: 1;
  height: 100%;
  background-color: #fff;
}
.search-row .close-circle {
  background-color: #fff;
  width: 110rpx;
  height: 100%;
  font-size: 26rpx;
  padding: 0;
  margin: 0 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
